{
    "name": "klib/jk-laravel-feapi",
    "description": "一个简单的支持RESTful接口的前后端组件模块，从JKGlib框架下移植到Laravel框架",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kami1983",
            "email": "meetcancanyou@yahoo.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "KLib\\FEApiLaravel\\": "src/FEApiLaravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "src/tests/",
            "App\\": "src/tests/app"
        }
    }
}
